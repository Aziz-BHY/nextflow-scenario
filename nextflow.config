projectDir= '$workflow.projectDir'


params {
  docker_image = "cned-scenario:latest"
}

params {
  // Note: These paths contains * characters to match all files
  // Note bis: Files should at least contain the pattern "_w[0-9]*.csv"
  train_data= "$projectDir/raw-data/trainData/logSP31_w*.csv"
  test_data= "$projectDir/raw-data/testData/SP31_w*.csv"
  true_labels="$projectDir/raw-data/testData/SP31_w*.csv"
  userModel=false
  userTest=false
}

includeConfig 'modules/Data_Request.config'
includeConfig 'modules/RFProcess.config'
includeConfig 'modules/predictionEvaluation.config'
includeConfig 'modules/testProcess.config'
includeConfig 'modules/aggregateResults.config'
