#!/usr/bin/env nextflow

process userModel {
    container = "{{ docker_image }}"

    cpus {{ cpu_limit|default(2, true) }}
    memory '{{ memory_limit|default("500 MB", true) }}'

    input:
        // input_file_1 is the path of the train data
        tuple val(index), path(input_file_1)

    output:
        tuple val(index), path(output_file_1)
"""
{{ command }}
"""
}
